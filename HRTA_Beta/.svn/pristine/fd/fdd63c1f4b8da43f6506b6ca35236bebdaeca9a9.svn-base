/*나눔웹폰트*/
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanumgothic/nanumgothic.css");
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanumbarungothic/nanumbarungothic.css");
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanumgothiccoding/nanumgothiccoding.css");
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanummyeongjo/nanummyeongjo.css");
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanumpenscript/nanumpenscript.css");
@import url("//cdn.jsdelivr.net/font-nanum/1.0/nanumbrushscript/nanumbrushscript.css");
@import url(//fonts.googleapis.com/css?family=Open+Sans:400,600,700);
/*reset*/
* {
	font-family: 'Nanum Barun Gothic', sans-serif;
 @include box-sizing(border-box);
}
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 12px;
	font-family: 'Open Sans','나눔바른고딕','NanumBarunGothic','Nanum Barun Gothic', sans-serif;
	-webkit-font-smoothing: antialiased;
	font-smoothing: antialiased;
	vertical-align: baseline;
	line-height: 1;
	color: #454545;
}
body {
	background: #fff;
}
caption, legend {
	font-size: 0;
	height: 0;
	line-height: 0;
	visibility: hidden;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
	width: 100%;
}
td input[type=text], td input[type=password] {
}
td input, td select {
	vertical-align: middle;
}
img {max-width: 100%;}
em {
	font-style: normal;
}
a {
	color: #333;
	text-decoration: none;
}
a:hover {text-decoration: none;}


select {
	line-height: 28px;
	border: 1px solid #c6c6c6;
	padding: 0 3%;
	height: 30px;
	vertical-align: middle;
}
select option {color:#6a6a6a; line-height:20px;}

input[type=submit], button {
	cursor: pointer;
}
input[type=text], input[type=password] {
	line-height: 28px;
	border: 1px solid #c6c6c6;
	padding: 0 3%;
	height: 30px;
	vertical-align: middle;
}


button {border: 0; background: none; padding: 0; margin: 0;}

::-ms-clear {
 display: none;
}

*, *:before, *:after {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}


#wrap {
	width: 100%;
}

#header {
	width: 100%;
	height: 255px;
	line-height: 50px;
	text-align: center;
	background: url('../images/head-bg.jpg') repeat-x center top;
}
#container {
	padding: 1%;
	background: #ddd;
}
#tabNav {
	overflow: hidden;
}
#tabNav li {
	float: left;
	margin: 0 .1%;
}
#tabNav li a {
	display: block;
	padding: 0 20%;
	height: 40px;
	line-height: 40px;
	background: #c8c8c8;
	border: 1px solid #c8c8c8;
}
#tabNav li a.active {
	background:#5a5a5a;
	color: #fff;
}

#body {
	
	overflow: hidden;
}
#breadCurmb {margin-bottom: 20px;}
#content {
	float: left;
	background: #fff;
	width: 84.5%;
}

#cols {
	padding: 3%;
}

#cols h1 {
	font-size: 24px;
	margin-bottom: 10px;
}
#cols h2 {
	font-size: 15px;
	margin-bottom: 10px;
}
#cols h3,#cols h4,#cols h5,#cols h6 {
	font-size: 13px;
	margin-bottom: 10px;
}
#cols h3 {margin-bottom: 30px;}
#cols h4, #cols h5, #cols h6 {color: #9b9b9b;}
#cols .hgroup {margin-bottom: 30px;}
#cols .desc {margin-bottom: 20px;}
#cols .act {margin-bottom: 45px;} 

.tNav{
	background: #fff;
	position:relative;
	margin:0 0;
	zoom:1;
	float: left;
	width: 15%;
	margin-right: .5%;
}
.tNav ul{
	list-style:none;
	margin:0;
	padding:0;
}
.tNav ul ul{margin:0 0 0 -3px}
.tNav li{
	position:relative;
	padding:0 0 0 0;
	white-space:nowrap;
	line-height:20px;
	*zoom:1
}

.tNav li.last{background-position:9px -1766px}
.tNav li.active span{
	font-weight:bold;
	background: #17a5f0;
	color: #fff;
	display: block;
	padding: 0 4%;
	height: 40px;
	line-height: 40px;
	font-size: 15px;
	text-align: left;
}
.tNav li.active li a{
	font-weight:normal;
	color:#767676
	
}
.tNav a{
	text-decoration:none;
	color:#767676;
	display: block;
	padding: 15px 10px 12px 25px;
	font-size: 15px;
}
.tNav a:hover,
.tNav a:active,
.tNav a:focus{text-decoration:underline}
.tNav .tNavToggle{
	position:absolute;
	top:0;
	left:0;
	width:19px;
	height:19px;
	padding:0;
	overflow:hidden;
	border:0;
	font-size:0;
	color:#fff;
	vertical-align:middle;
	text-indent:19px;
	*text-indent:0;
	background:transparent url(../images/btnPlusMinus.gif) no-repeat;
	cursor:pointer
}
.tNav li ul {
	padding: 2% 0 2% 10%;
}
.tNav li ul li a {
	display: block;
	background:#fff url(../images/bg_nav_sub.png) 0 50% no-repeat;
	line-height: 30px;
	padding: 0 0 0 14px;
	color: #717171;
	font-size: 14px;
}
.tNav li.active ul li a {
	background:#fff url(../images/bg_nav_sub.png) 0 50% no-repeat;
}
.tNav li.active ul li a.on {
	background:#fff url(../images/bg_nav_sub_on.png) no-repeat 0 50%;
	color: #596bd6;}

.tNav .tNavToggle.plus{background-position:5px -15px}
.tNav .tNavToggle.minus{background-position:5px 5px}


table.styledLeft, table.styledVertical {
	border-top: 2px solid #c2c2c2;
	border-bottom: 1px solid #c2c2c2;
}
table.styledLeft tbody th, table.styledLeft tbody td {
	text-align: left;
	padding: 1%;
	font-weight: normal;
}
table.styledLeft tbody td {border-left: 1px solid #a7a7a7;}
table.styledLeft tbody tr.tableEvenRow th, table.styledLeft tbody tr.tableEvenRow td {background: #ececec;}
table.styledVertical {
	border-right: 1px solid #a7a7a7;
	border-left: 1px solid #a7a7a7;
}
table.styledVertical thead th, table.styledVertical tbody th {font-weight: normal;}
table.styledVertical thead th {
	padding: 1%;
	background: #7592ED;
	border-bottom: 1px solid #a7a7a7;
	border-left: 1px solid #a7a7a7;
	color:#fff;
	text-align: center;
}
table.styledVertical tbody th, table.styledVertical tbody td {
	padding: 1%;
	text-align: left;
	border-bottom: 1px solid #a7a7a7;
	border-left: 1px solid #a7a7a7;
	border-right: 1px solid #a7a7a7;
	vertical-align: middle;
}
table.styledVertical tbody th a {
	color: #008acc;
	text-decoration: underline;
}
table.styledVertical tbody td.fnc-btn a, table.styledVertical tbody td.fnc-btn button {
	display: block;
	font-size: 13px;
	line-height: 28px;
	border: 1px solid #c6c6c6;
	padding: 0 3%;
	width: auto;
	text-align: center;
	width: 130px;
	margin: 1.8% auto 1.8% auto;
	background: #f9f9f9;
}
table.styledVertical tbody td.fnc-btn a:hover, table.styledVertical tbody td.fnc-btn button:hover {background: #efefef;}
table.styledVertical tbody td.fnc-btn i {margin: 0 5px 0 0;}
table.styledVertical tbody td.fnc-btn .statics  i {color: #f84b07;}
table.styledVertical tbody td.fnc-btn .tracing  i {color: #f8c807;}
table.styledVertical tbody td.fnc-btn .edit  i {color: #101bb0;}
table.styledVertical tbody td.fnc-btn .delete  i {color: #ca000a;}
table.styledVertical td 
.lead-t {line-height: 24px;}

