.line-frm {padding: 3px 0;}

#login {
	background:url('../images/lgoin.jpg') no-repeat 0 0;
	width: 1200px;
	margin: -255px 0 0 -600px;
	padding: 266px 349px 70px 99px;
	position: absolute;
	top: 50%;
	left: 50%;
}
#login .login-frm {
	background: #fff;
	padding: 20px;
	width: 752px;
	border-radius: 10px;
	position: relative;
}
#login .login-frm h1 {
	font-size: 30px;
	margin: 0 0 20px 0;
	color: #2b77b5;
}
#login .login-frm label, #login .login-frm input {
	float: left;
	height: 30px;
	line-height: 30px;
	font-size: 18px;
}
#login .login-frm p {
	overflow: hidden;
	width: 80%;
	margin: 5px 0;
}
#login .login-frm p .save {
	font-size: 12px;
	line-height: 30px;
	float: left;
	padding: 0 0 0 5px;
}
#login .login-frm p .save input {
	padding: inherit;
	width: inherit;
	height: inherit;
	line-height: 21px;
	float: none;
	vertical-align: middle;
	margin: 0;
}
#login .login-frm input {
	border: 1px solid #c3c3c3;
	border-radius: 5px;
	width: 50%;
}
#login .login-frm label {
	width: 100px;
}
#login .login-frm .btn-login {
	position: absolute;
	top: 105px;
	left: 410px;
	width: auto;
}
#login .login-frm .btn-login button {
	background: #1d5a9c;
	color: #fff;
	height: 30px;
	line-height: 30px;
	width: 100px;
	font-size: 18px;
	border-radius: 5px;
	
}
.line-tb {
	border-top: 2px solid #c2c2c2;
}
.line-tb thead th {
	padding: 1%;
